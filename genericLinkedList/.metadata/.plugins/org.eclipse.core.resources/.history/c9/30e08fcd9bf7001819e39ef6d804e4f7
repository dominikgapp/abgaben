package generischeLinkedList;

public class Node <T> {

	private T value; 
	private T next = null; 
	
	public Node(T value)
	{
		this.value = value; 
	}
	
	public T getValue ()
	{
		return this.value; 
	}
	
	public void setValue(T x)
	{
		this.value = x; 
	}
	
	public T getNext()
	{
		return next; 
	}
	
	public void setNext (T next)
	{
		this.next = next; 
	}
}
